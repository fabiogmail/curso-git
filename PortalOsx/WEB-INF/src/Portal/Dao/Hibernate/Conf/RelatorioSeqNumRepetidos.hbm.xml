<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Portal.Dto.NumRepetidos" table = "seq_num_repetidos">
        <id name="id" column="ID_SEQ_NUM_REPETIDOS" type="int" />
        
        <property name="idBilhetador" column="ID_BILHETADOR"/>
        <property name="data" column="DATA"/>
        
		<property name="numerosRepetidos" column="SEQ_NUM_REPETIDOS"/> 
		<property name="qtdSeqNumerosRepetidos" column="QTD_SEQ_NUM_REPETIDOS"/>
			
    
    </class>
</hibernate-mapping>

